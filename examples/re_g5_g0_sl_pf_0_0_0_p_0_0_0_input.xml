    <threept_multi_alt_exp_expE_model>  <!-- this is for the B to K 3-point function at p_F=0 -->
      <n_exp_initial>1</n_exp_initial>
      <n_o_exp_initial>1</n_o_exp_initial>
      <n_exp_final>1</n_exp_final>
      <n_o_exp_final>0</n_o_exp_final>
